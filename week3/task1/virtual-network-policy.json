{
    "policyRule": {
      "if": {
        "allOf": [
          {
            "field": "type",
            "equals": "Microsoft.Network/virtualNetworks"
          },
          {
            "field": "name",
            "notLike": "*-test-vnet"
          },
          {
            "field": "name",
            "notLike": "*-qa-vnet"
          },
          {
            "field": "name",
            "notLike": "*-prod-vnet"
          }
        ]
      },
      "then": {
        "effect": "audit"
      }
    }
  }